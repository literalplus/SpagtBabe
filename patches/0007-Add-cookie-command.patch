---
 plugins/cookies.py | 17 +++++++++++++++++
 1 file changed, 17 insertions(+)
 create mode 100644 plugins/cookies.py

diff --git a/plugins/cookies.py b/plugins/cookies.py
new file mode 100644
index 0000000..ac62ffd
--- /dev/null
+++ b/plugins/cookies.py
@@ -0,0 +1,17 @@
+from util import hook
+
+@hook.command
+@hook.command("share")
+@hook.command("cookies")
+def cookie(inp, me=None, nick=None):
+  """cookie -- Shares a cookie with you
+  cookie <User> -- Shares a cookie with <person>"""
+  
+  target = inp.strip()
+  
+  if target == "":
+    me("shares a cookie with " + nick)
+    return
+  
+  me("shares a cookie with " + target)
+  
\ No newline at end of file
